{
  "activeProvider": "ollama",
  "fallbackProvider": null,
  "providers": {
    "ollama": {
      "enabled": true,
      "baseUrl": "http://localhost:11434",
      "model": "phi3:mini",
      "temperature": 0.1,
      "maxTokens": 4096,
      "streaming": false
    },
    "gemini": {
      "enabled": true,
      "useAuth": true,
      "model": "gemini-2.0-flash-exp",
      "temperature": 0.1,
      "maxTokens": 4096
    },
    "openai": {
      "enabled": false,
      "apiKey": "env:OPENAI_API_KEY",
      "model": "gpt-4o-mini",
      "temperature": 0.1,
      "maxTokens": 4096
    },
    "anthropic": {
      "enabled": false,
      "apiKey": "env:ANTHROPIC_API_KEY",
      "model": "claude-3-5-sonnet-20241022",
      "temperature": 0.1,
      "maxTokens": 4096
    }
  },
  "mcp": {
    "servers": {
      "chrome-devtools": {
        "command": "npx",
        "args": ["chrome-devtools-mcp"],
        "env": {
          "CHROME_PATH": "C:\\Program Files\\Google\\Chrome\\Application\\chrome.exe"
        }
      }
    }
  },
  "testing": {
    "browser": "chromium",
    "headless": false,
    "viewport": {
      "width": 1920,
      "height": 1080
    },
    "defaultTimeout": 30000,
    "screenshotOnError": true,
    "videoOnFailure": false,
    "retryFailedTests": 1,
    "continueOnFailure": true
  }
}